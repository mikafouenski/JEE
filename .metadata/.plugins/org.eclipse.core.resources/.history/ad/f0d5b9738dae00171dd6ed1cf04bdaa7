package exception;

public class ExceptionPersonInvalidate extends Exception {
	private String isNomValide;
	private String isEmailValide;
	
	public ExceptionPersonInvalidate() {
		// TODO Auto-generated constructor stub
		this.isEmailValide = "";
		this.isNomValide = "";
	}

	public String getIsNomValide() {
		return isNomValide;
	}

	public void setIsNomValide(String isNomValide) {
		this.isNomValide = isNomValide;
	}

	public String getIsEmailValide() {
		return isEmailValide;
	}

	public void setIsEmailValide(String isEmailValide) {
		this.isEmailValide = isEmailValide;
	}
		
}
