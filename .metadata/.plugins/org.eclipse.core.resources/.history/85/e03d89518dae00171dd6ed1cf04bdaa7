package exception;

public class ExceptionPersonInvalidate extends Exception {
	private String isNomValide;
	private String isEmailValide;
	
	public ExceptionPersonInvalidate() {
		// TODO Auto-generated constructor stub
		this.isEmailValide = "";
		this.isNomValide = "";
	}
	
	public boolean isNomValide() {
		return isNomValide;
	}
	public void setNomValide(boolean isNomValide) {
		this.isNomValide = isNomValide;
	}
	public boolean isEmailValide() {
		return isEmailValide;
	}
	public void setEmailValide(boolean isEmailValide) {
		this.isEmailValide = isEmailValide;
	}
	
	
	
}
